    "express-fileupload": "^1.4.0",
    "express-handlebars": "^7.0.4",
    "express-rate-limit": "^6.7.0",
    "helmet": "^7.0.0",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0",
    "memory-cache": "^0.2.0",
    "mongoose": "^7.0.3",
    "mongoose-paginate-v2": "^1.7.1",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.1",
