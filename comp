# Create a new completion function
_my_completion() {
    local cur opts
    COMPREPLY=()
    cur="${COMP_WORDS[COMP_CWORD]}"
    opts="make clear test key doc performance db search"
    COMPREPLY=( $(compgen -W "$opts" -- "$cur") )
    return 0
}

# Register the completion function
complete -F _my_completion artisan
